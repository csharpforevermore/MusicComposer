        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MattCallawayFinalProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="1032.352" Width="1102.075" Background="#FFD6D3D7">
    <Grid Margin="0,0,359.333,-18.667">

        <Button x:Name="tunerButton" Content="Tune Guitar" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="217" Margin="784,0,-303,917" Height="87" FontSize="25" Click="newTunerButtonClick" Grid.Column= "1" Background="#FF8AB671"/>
        <Image HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Margin="901,317,-289,0"/>
        <TextBox HorizontalAlignment="Left" Height="22" TextWrapping="Wrap" VerticalAlignment="Top" Width="634" Margin="71,35,0,0" Panel.ZIndex="3"/>
