            <Image x:Name="sheetImage" Height="1032" Width="724" Panel.ZIndex="-1"/>
            <Button Content="Add a Note" Click="AddButtonClick" FontSize="25" Height="90" Canvas.Left="784" Canvas.Top="382" Width="217" Background="#FFADC8D1"/>
            <TextBox HorizontalAlignment="Left" Height="22" TextWrapping="Wrap" VerticalAlignment="Top" Width="634" Panel.ZIndex="3" Canvas.Left="71" Canvas.Top="529"/>
            <TextBox HorizontalAlignment="Left" Height="22" TextWrapping="Wrap" VerticalAlignment="Top" Width="634" Panel.ZIndex="3" Canvas.Left="71" Canvas.Top="756"/>
            <TextBox HorizontalAlignment="Left" Height="22" TextWrapping="Wrap" VerticalAlignment="Top" Width="634" Panel.ZIndex="3" Canvas.Left="71" Canvas.Top="868"/>
            <TextBox HorizontalAlignment="Left" Height="22" TextWrapping="Wrap" VerticalAlignment="Top" Width="634" Panel.ZIndex="3" Canvas.Left="71" Canvas.Top="974"/>
        </Canvas>
        <TextBox HorizontalAlignment="Left" Height="22" TextWrapping="Wrap" VerticalAlignment="Top" Width="634" Margin="71,300,0,0" Panel.ZIndex="3"/>
        <TextBox HorizontalAlignment="Left" Height="22" TextWrapping="Wrap" VerticalAlignment="Top" Width="634" Margin="71,416,0,0" Panel.ZIndex="3"/>
